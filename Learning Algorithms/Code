    "X_train_1 ,X_test_1, y_train_1 , y_test_1 = train_test_split(X_sc, Y1, test_size = 0.3 , random_state = 29)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 37,
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.neural_network import MLPClassifier\n",
    "\n",
    "## Making use of different hidden layers\n",
    "layer_size = np.arange(1,40,2)\n",
    "train_accuracy = []\n",
    "test_accuracy = []\n",
    "for i in layer_size:\n",
    "    ANN = MLPClassifier(hidden_layer_sizes= (i), activation='relu', alpha=0.0001,\n",
    "                              solver= 'adam', max_iter = 200, random_state=11)\n",
    "    ANN.fit(X_train_1, y_train_1)\n",
    "    y_pred_train = ANN.predict(X_train_1)\n",
    "    y_pred_test = ANN.predict(X_test_1)\n",
    "    train_accuracy.append(accuracy_score(y_train_1,y_pred_train))\n",
    "    test_accuracy.append(accuracy_score(y_test_1,y_pred_test))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 38,
   "metadata": {},
   "outputs": [
